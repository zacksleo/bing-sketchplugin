{
  "name": "bing",
  "version": "1.0.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "manifest": "src/manifest.json",
    "main": "bing.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.10",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "author": "zacksleo",
  "repository": "https://github.com/zacksleo/bing-sketchplugin.git",
  "description": "Easily grab images from Bing",
  "license": "MIT",
  "dependencies": {
    "@skpm/fs": "^0.2.0"
  }
}
